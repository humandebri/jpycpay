{
  "version": 1,
  "dfx": "0.24.3",
  "canisters": {
    "relayer": {
      "type": "rust",
      "package": "relayer",
      "candid": "canisters/relayer/relayer.did",
      "wasm": "target/wasm32-unknown-unknown/release/relayer.wasm",
      "build": [
        "cargo",
        "build",
        "--target",
        "wasm32-unknown-unknown",
        "--release",
        "--package",
        "relayer"
      ]
    },
    "jpyc_wrapper": {
      "type": "motoko",
      "main": "canisters/jpyc_wrapper/main.mo"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral"
    }
  }
}
